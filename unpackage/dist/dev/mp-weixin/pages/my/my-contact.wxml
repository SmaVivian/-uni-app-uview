<view class="page-my-contact"><view class="list"><block wx:for="{{personList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['checkAddress',['$0'],[[['personList','',index]]]]]]]}}" class="item" bindtap="__e"><text data-event-opts="{{[['tap',[['addAddress',['edit','$0'],[[['personList','',index]]]]]]]}}" catchtap="__e">编辑</text><view class="person-wrap u-margin-left-10"><view class="title-box"><block wx:if="{{item.default}}"><text class="tag">默认</text></block><text class="person">{{item.name}}</text></view><view class="info-box"><text>{{item.phone+"}"}}</text></view></view><u-icon vue-id="{{'b32500e6-1-'+index}}" name="{{item.checked?'checkmark-circle-fill':'checkmark-circle'}}" color="#bd1b2d" size="40" data-event-opts="{{[['^click',[['toggleCheck',['$0'],[[['personList','',index]]]]]]]}}" bind:click="__e" bind:__l="__l"></u-icon></view></block></view><view class="u-margin-top-40"><u-button vue-id="b32500e6-2" type="primary" shape="circle" data-event-opts="{{[['^click',[['addAddress',['add']]]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">新增联系人</u-button></view><view class="u-margin-top-40"><u-button vue-id="b32500e6-3" type="primary" shape="circle" data-event-opts="{{[['^click',[['sure']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">确定</u-button></view></view>