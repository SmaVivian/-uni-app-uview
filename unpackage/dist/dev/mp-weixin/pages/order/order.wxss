.page-order .content {
  padding: 20px 16px;
  -webkit-box-shadow: 0px 3px 10px 0px rgba(173, 179, 191, 0.3);
          box-shadow: 0px 3px 10px 0px rgba(173, 179, 191, 0.3);
  -webkit-border-radius: 8px;
          border-radius: 8px;
}
.page-order .content ._h2 {
  padding-bottom: 20px;
  font-size: 16px;
  border-bottom: dashed 1px #bcbbd5;
  color: #262628;
}
.page-order .content ._h2 ._span {
  font-size: 12px;
  color: #565862;
}
.page-order .content ._ul ._li {
  position: relative;
  line-height: 36px;
  padding-right: 60px;
  font-size: 14px;
  color: #565862;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: dashed 1px #bcbbd5;
}
.page-order .content ._ul ._li .name {
  display: inline-block;
  width: 80px;
  font-size: 16px;
  color: #262628;
}
.page-order .content ._ul ._li .btns {
  position: absolute;
  width: 60px;
  right: 0;
  top: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 100%;
  padding-left: 5px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.page-order .content ._ul ._li .btns .btn {
  display: inline-block;
  width: 16px;
  height: 16px;
  padding: 3px;
}
.page-order .content ._ul ._li .btns .btn-edit {
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 16px 16px;
  margin-right: 4px;
}
.page-order .content ._ul ._li .btns .btn-dele {
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 16px 16px;
}
.page-order .content .g-btn-primary {
  display: inline-block;
}
.page-order .content .tips {
  text-align: center;
  font-size: 12px;
  color: #949397;
}

